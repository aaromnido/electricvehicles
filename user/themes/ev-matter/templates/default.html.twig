{% extends 'partials/base.html.twig' %}

{% block content %}
{% set image = page.media.images|last %}

{% if image %}
	<div class="section-header-picture">
		{{ image.cropZoom(1280, 340).html }}
	</div>
{% endif %}

	<div id="content">
		<h1 class="title">{{ page.title }}</h1>
		{{ page.content }}
	</div>
{% endblock %}
